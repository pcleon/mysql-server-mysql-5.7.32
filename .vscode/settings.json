{
    "python.defaultInterpreterPath": "/usr/bin/python",

    "cmake.configureSettings": {
        "CMAKE_BUILD_TYPE": "RelWithDebInfo",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "YES",
        "CMAKE_VERBOSE_MAKEFILE": "YES"
    },
    "cmake.configureArgs": [
        "-DWITH_BOOST=/home/leon/dbug/boost_1_59_0",
        "-DDOWNLOAD_BOOST=0",
        "-DWITH_JEMALLOC=1",
        "-DWITH_SSL=/usr/local/openssl-1.1.1w",
        "-DCMAKE_INSTALL_PREFIX=/home/leon/dbug/mysql5732",

    ],
    "cmake.buildDirectory": "${workspaceFolder}/cmake-build-debug/build",
    "cmake.buildArgs": [
        "--parallel"
    ],
    "cmake.parallelJobs": 4,
    "files.associations": {
        "*.py ": "python",
        "cstdlib": "cpp",
        "*.ipp": "cpp"
    },
    "commentTranslate.source": "DarkCWK.youdao-youdao",
 
}